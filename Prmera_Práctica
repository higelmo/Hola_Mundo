/* Comentario de varias líneas
*/
//Comentario para una sola línea//
// Los tipos de datos más utilizados: int;string,char,double//

package priemer_proyecto;


public class Priemer_Proyecto {

    
    public static void main(String[] args) {
        
// Creación de la variable tres, que almacena un String/        
        String tres = "20";
//Almacenar en cuatro la variable tres pero de tipo integer, en vez de String // 
        int cuatro = Integer.parseInt(tres); 
 //Cración de una variable llamada uno que almacena un entero//
        int uno = 35; 
 //Creación de una variable llamada dos que almacena un decimal//       
        double dos = 89;
 //Creación de una variable llamada suma que almacena un decimal//
        double suma = uno + dos;
        int x = cuatro + uno;
 //Impresión en pantalla//
        System.out.println(cuatro);
        System.out.println("La suma de las variables cuatro y uno es");
        System.out.println(x);
      
    }
    
}





